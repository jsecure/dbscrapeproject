<div class="jumbotron text-center">
  <div class="overlay">
  </div>
  <div class="background-image">
  </div>
  <div class="caption">
    <h1>Mongo Scraper</h1>
    <p>New York Times Edition</p>
  </div>
</div>
<ul class="container-fluid article-container">  
  {{#each articles}}
<li data-_id="{{this._id}}" class="card">
    <div>{{this.headline}}</div>
    <div class="list-summary">{{this.summary}}</div>
 </li>
  {{/each}}
</ul>

